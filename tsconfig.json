{
  "compilerOptions": {
    "outDir": "./",
    "allowJs": true,
    "strict": true,
    "module": "es6",
    "lib": ["ES2017"],
    "types": ["@types/node", "@citizenfx/server", "@citizenfx/client"],
    "target": "es6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strictNullChecks": false,
    "noFallthroughCasesInSwitch": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "include": ["./src/**/*.ts"],
  "exclude": []
}